{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ],
  "env": {
    "dist": {
      "plugins": [
        [
          "replace-require-suffix",
          {
            "from": "scss",
            "to": "css"
          }
        ]
      ]
    }
  }
}
