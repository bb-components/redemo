{
  "name": "redemo",
  "version": "1.0.4",
  "description": "react demo component",
  "keywords": [
    "react",
    "demo",
    "component",
    "doc",
    "redemo"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open",
    "doc:pub": "NODE_ENV=production webpack --config webpack-dist.config.js && rm -rf .doc",
    "npm:scss": "node-sass src/ -o .npm/",
    "npm:js": "BABEL_ENV=dist babel src/ --out-dir .npm/ --source-maps",
    "npm:pub": "npm run npm:scss && npm run npm:js && cp ./package.json .npm/ && cp -R ./src/iconfont ./.npm/iconfont && npm publish .npm/ && rm -rf .npm"
  },
  "author": "halwu",
  "license": "ISC",
  "dependencies": {
    "antd": "^2.10.2",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-highlight": "^0.10.0",
    "remarkable": "^1.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-replace-require-suffix": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.2",
    "docgen-loader": "^1.3.4",
    "end-webpack-plugin": "^1.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^1.0.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.3",
    "react-docgen": "^3.0.0-beta4",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.0",
    "web-webpack-plugin": "^1.5.8",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  }
}
