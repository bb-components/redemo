{
  "name": "redemo",
  "version": "0.0.6",
  "description": "react demo component",
  "keywords": [
    "react",
    "demo",
    "component",
    "doc",
    "redemo"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config doc-webpack.config.js --open",
    "doc:pub": "rm -rf doc && NODE_ENV=production webpack --config doc-webpack-dist.config.js",
    "npm:scss": "node-sass src/ -o npm/",
    "npm:js": "BABEL_ENV=dist babel src/ --out-dir npm/ --source-maps",
    "npm:umd": "NODE_ENV=production webpack --config npm-webpack.config.js",
    "npm:pub": "rm -rf npm && npm run npm:scss && npm run npm:js && npm run npm:umd && npm publish npm/"
  },
  "author": "halwu",
  "license": "ISC",
  "dependencies": {
    "antd": "^2.9.3",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-highlight": "^0.9.0",
    "react-remarkable": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-replace-require-suffix": "^1.0.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.0",
    "docgen-loader": "^1.3.4",
    "end-webpack-plugin": "^1.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "gh-pages": "^0.12.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.2",
    "react-docgen": "^2.14.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "web-webpack-plugin": "^1.5.5",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  }
}
